<!doctype html>
<html lang="en" ng-app>
  <head>
    <title>Calli</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    
    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
    
    <!-- Latest compiled and minified JavaScript -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="/css/index.css">
  </head>
  <body>
      
    <%- include nav.ejs %>
      
    <div class="container">
      <div class="date panel panel-default">
          <div class="panel-body">
            <span class="mr-auto">
              <button class="btn " onclick="window.location.href='#'">오늘</button>
              <button class="btn " onclick="window.location.href='#'">1주일</button>
              <button class="btn " onclick="window.location.href='#'">1개월</button>
              <button class="btn " onclick="window.location.href='#'">3개월</button>
              <button class="btn " onclick="window.location.href='#'">6개월</button>
              <button class="btn " onclick="window.location.href='#'">12개월</button>
            </span>
            <form action="#" method="POST" class="form-inline">
            <div class="form-group">
              <label class="sr-only" for="start">start date</label>
              <input type="date" name="startdate" class="form-control" id="start">
            </div>
            ~
            <div class="form-group">
              <label class="sr-only" for="end">end date</label>
              <input type="date" name="enddate" class="form-control" id="end">
            </div>
            <button type="submit" class="btn btn-default">Search</button>
            </form>
          </div>
      </div>
      
      <button onclick="location.href='#'" type="button" class="btn">AD뷰 Best</button>
      <button onclick="location.href='#'" type="button" class="btn">다운수 Best</button>

      <div class="explan">
        <ul>
          <li>총 <%= all %>명 (남자 <%= male %>명 / 여자 <%=female%>명 | Android <%=android%>명 / ios <%=ios%>명)</li>
        </ul>
      </div>
      
      <div class="img_list">
        <table class="table table-hover">
          <thead>
            <td>No</td>
            <td>가입일</td>
            <td>최근 로그인</td>
            <td>기기</td>
            <td>계정</td>
            <td>이메일</td>
            <td>성별</td>
            <td>나이</td>
            <td>푸시 시간</td>
            <td>방문수</td>
            <td>다운수</td>
            <td>별</td>
            <td>AD뷰</td>
          </thead>
          <% for(var i in user){ %>
            <tr>
              <td><%= user[i].id %></td>
              <td><%= user[i].created_at %></td>
              <td><%= user[i].logined_at %></td>
              <td><%= user[i].device %></td>
              <td><%= user[i].account_type %></td>
              <td><%= user[i].email %></td>
              <td><%= user[i].gender %></td>
              <td><%= user[i].age %></td>
              <td><%= user[i].push_time %></td>
              <td><%= user[i].visit %></td>
              <td><%= user[i].down %></td>
              <td><%= user[i].star %></td>
              <td><%= user[i].ADview %></td>
            </tr>
          <% } %>
          
        </table>
      </div>
      
      <div class="col-md-1 col-md-offset-11 col-sm-1 col-sm-offset-11">
        <button onclick="location.href='#'" type="button" class="btn btn-primary">
          엑셀파일로 저장
        </button>
      </div>
    </div>
    
  </body>
</html>
